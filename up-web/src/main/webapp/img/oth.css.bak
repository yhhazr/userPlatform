body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button, input, textarea, th, td { margin:0; padding:0;}
body, button, input, select, textarea { font:12px / 1.5 tahoma, arial, \5b8b\4f53; }
h1, h2, h3, h4, h5, h6 { font-size:100%; }
ul li{ list-style:none; }
img{border:0px;}
h2{ padding-bottom:16px; width:545px; font-size:14px; color:#060606; font-weight:normal; border-bottom:1px dashed #c5c5c5;}
h3{ font-size:16px; color:#ff6800;}
h4{ padding:20px 0 0 12px; color:#555555; line-height:24px; font-weight:normal;}
h5{ margin:210px 0 0 50px;}
h6{ padding:20px 0 0 0; font-weight:normal; color:#232323;}
h6 a{ color:#0078ff; padding:20px 0 0 0; text-decoration:none; }
h6 a:hover{ color:#0078ff; padding:20px 0 0 0; text-decoration:underline; }
p{ padding:40px 0 0 0;}
.box,.topNav,.clearfix,.main,.footer{zoom:1;}
.box:after,.topNav:after,.clearfix:after,.main:after,.footer:after{ content:" "; clear: both; zoom:1; height:1%; display: block;}
.box,.topNav,.main,.footer{ width:1000px; margin:0 auto;}
.czcg{ background:url(czcg.jpg) no-repeat; width:487px; height:282px; position:absolute; z-index:1002;}
.close{ float:right; padding: 12px 8px 0 0; font-size:12px; color:#321701; }
.close a{ padding: 12px 8px 0 0; font-size:12px; color:#321701; text-decoration:none; }
.close a:hover{ padding: 12px 8px 0 0; font-size:12px; color:#321701; text-decoration:underline; }
.cz-contant{ padding:68px 0 0 0; position:absolute; left:42px;}
.czqr{ background:url(czqr.jpg) no-repeat; width:487px; height:323px; position:absolute; z-index:1002;}
.qr-contant{ padding:60px 0 0 0; position:absolute; left:32px;}
.qr-contant ul{ list-style-type:none; display:block;width:250px;}
.qr-contant ul li{  color:#555555; line-height:33px;}
.fnt_spe{ color:#fe6700; font-weight:bold;}
.czyc{ width:707px; height:460px; background-color:#fffcf1; border:1px solid #f9dfce; position:relative;}
.err_ties{ padding:18px 0 0 40px; height:42px; font-size:16px; color:#232323; font-weight:bold;}
.err_ties img{ margin:0 0 -16px 0;}
.err_msg{ width:578px; height:327px; padding:20px 0 20px 34px; border:1px solid #dadada; background-color:#ffffff; position:absolute; left:42px; top:76px; }
.err_msg ul{ display:block; padding:14px 0 0 0;}
.err_msg ul li{ color:#757474; line-height:26px;}
.err_fnt{ color:#ff0000; font-weight:bold;}
.black_overlay {
 display: none;
 position: absolute;
 top: 0%;
 left: 0%;
 width: 100%;
 height: 100%;
 background-color:#f5f5f5;
 z-index:1001;
 -moz-opacity: 1.0;
 opacity:.80;
 filter: alpha(opacity=100);
}